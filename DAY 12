///////////////////CODE 1
#include <iostream>
using namespace std;

int main() {
	{
	    int t;
	    cin>>t;
	    for(int i=1;i<=t;i++)
	    {
	        int x,y,a,b;
	        cin>>a>>b>>x>>y;
	        int t=a*b;
	        int c=x*y;
	        if(c>=t)
	        {
	            cout<<"yes"<<endl;
	        }
	        else
	        {
	            cout<<"no"<<endl;
	        }
	        
	    }
	    
	}
	return 0;
}
