//////////////////////////////////CODE 1////////
#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    if(n<=10)
	         {
	             cout<<"lower double"<<endl;
	         }
	         else if(n>10 && n<=15)
	         {
	             cout<<"lower single"<<endl;
	         }
	        else if(n>=16 && n<=25)
	        {
	            cout<<"upper double"<<endl;
	        }
	        else if(n>=26 && n<=30)
	        {
	            cout<<"upper single"<<endl;
	        }
	    
	}
	return 0;
}
