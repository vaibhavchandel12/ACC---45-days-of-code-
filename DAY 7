////////////////////////////////CODE 1////////////
#include <iostream>
using namespace std;

int main() 
{
	int t ;
	cin>>t;
	for(int i=1;i<=t;i++)
	{
	    int x,y;
	    cin>>x>>y;
	    if(x>(10*y))
	    {
	        cout<<"yes"<<endl;
	    }
	    else
	    {
	        cout<<"no"<<endl;
	    }
	}
	return 0;
}
/////////////////////////////CODE2////////////////////////
#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	for(int i=1;i<=t;i++)
	{
	    int a,b,c,t;
	    cin>>a>>b>>c;
	    t=a+c;
	    if(t>b)
	    {
	        cout<<t<<endl;
	    }
	    else
	    {
	        cout<<b<<endl;
	    }
	}
	return 0;
}

